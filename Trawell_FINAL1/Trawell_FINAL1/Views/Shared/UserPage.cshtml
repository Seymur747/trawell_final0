                                <button class="navbar-toggler navbar-toggler-right border-0 " type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                                    <span class="fa fa-bars fa-lg text-white"></span>
                                </button>
                            </div>

                            <div class="collapse navbar-collapse" id="navbarNav">
                                <ul class="navbar-nav">
                                    <li class="nav-item active color-second">
                                        <a class="nav-link" href="~/Areas/User/User_Front/Travell-master/index.html">Home<span class="sr-only">(current)</span></a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#">Flights</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link " href="~/Areas/User/User_Front/Travell-master/rentalsSearch.html">Rentals</a>
